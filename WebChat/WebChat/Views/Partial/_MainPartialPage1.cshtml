@model IEnumerable<WebChat.Models.CustomModel.CustomChatRoom>


<div class="middle">
    <h4>Cuộc trò chuyện</h4>
    <button type="button" class="btn round" data-toggle="modal" data-target="#compose"><i data-eva="edit-2"></i></button>
    <hr>
    <ul class="nav discussions" role="tablist">
        <li>
            <a href="#chat1" class="filter direct active" data-chat="open" data-toggle="tab" role="tab" aria-controls="chat1" aria-selected="true">
                <div class="status online"><img src="~/Content/img/avatars/avatar-male-1.jpg" alt="avatar"><i data-eva="radio-button-on"></i></div>
                <div class="content">
                    <div class="headline">
                        <h5>Ninh Thái Phan</h5>
                        <span>Hôm nay</span>
                    </div>
                    <p>Kiểm tra lại phần Đăng nhập và Đăng ký</p>
                </div>
            </a>
        </li>
        <li>
            <a href="#chat1" class="filter direct" data-chat="open" data-toggle="tab" role="tab" aria-controls="chat1" aria-selected="false">
                <div class="status offline"><img src="~/Content/img/avatars/avatar-male-2.jpg" alt="avatar"><i data-eva="radio-button-on"></i></div>
                <div class="content">
                    <div class="headline">
                        <h5>Nguyễn Minh Hiếu</h5>
                        <span>Hôm nay</span>
                    </div>
                    <p>Thêm dữ liệu</p>
                </div>
            </a>
        </li>
        <li>
            <a href="#chat1" class="filter direct" data-chat="open" data-toggle="tab" role="tab" aria-controls="chat1" aria-selected="false">
                <div class="status online"><img src="~/Content/img/avatars/avatar-male-3.jpg" alt="avatar"><i data-eva="radio-button-on"></i></div>
                <div class="content">
                    <div class="headline">
                        <h5>Triệu Quang Minh</h5>
                        <span>09/10</span>
                    </div>
                    <p>Thêm thư viện cho dự án</p>
                </div>
            </a>
        </li>
        <li>
            <a href="#chat1" class="filter direct" data-chat="open" data-toggle="tab" role="tab" aria-controls="chat1" aria-selected="false">
                <div class="status offline"><img src="~/Content/img/avatars/avatar-male-4.jpg" alt="avatar"><i data-eva="radio-button-on"></i></div>
                <div class="content">
                    <div class="headline">
                        <h5>Lê Sỹ Tùng</h5>
                        <span>08/10</span>
                    </div>
                    <p>Thay đổi đường dẫn cho file</p>
                </div>
            </a>
        </li>
        <li>
            <a href="#chat1" class="filter direct" data-chat="open" data-toggle="tab" role="tab" aria-controls="chat1" aria-selected="false">
                <div class="status offline"><img src="~/Content/img/avatars/avatar-male-5.jpg" alt="avatar"><i data-eva="radio-button-on"></i></div>
                <div class="content">
                    <div class="headline">
                        <h5>Nguyễn Nam</h5>
                        <span>07/10</span>
                    </div>
                    <p>Cài đặt lại các thông số</p>
                </div>
            </a>
        </li>
        @foreach (var item in Model)
        {
            <li>
                <a href="#chat2" class="filter groups" data-chat="open" data-toggle="tab" role="tab" aria-controls="chat2" aria-selected="false">
                    <div class="content">
                        <div class="headline">
                            <img src="~/Content/img/avatars/avatar-group-1.jpg" alt="avatar">
                            <h5>@item.RoomName</h5>
                            <span>Hôm nay</span>
                        </div>
                        @*<p>Commit all </p>*@
                    </div>
                </a>
            </li>
        }

    </ul>
</div>


